
# print first ten natural number
for i in range(1,11):
     print("natural nos ", i, "is : " ,i)

#  print first ten even nos
print("first 10 even nos.") 
for i in range(1,21,):
     if(i%2==0):
      print(i)


#  print first ten odd nos .......here 3rd 2 means 2 nos gap taken in between each nos in range
print("1-19 odd nos")
for i in range(1,21,2) :   
    print(i)


    
    
